{
  "defaultSeverity": "warn",
  "extends": [
    "tslint:recommended",
    "tslint-config-security"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": {
      "options": [
        140
      ]
    },
    "member-access": false,
    "quotemark": [
      true,
      "single"
    ],
    "space-before-function-paren": false,
    "tsr-detect-html-injection": false,
    "trailing-comma": false
  },
  "rulesDirectory": []
}
